{
  "collection": "development",
  "docker_compose_files": [
    "./docker-compose.yml"
  ],
  "initFolders": [
    "./data/.well-known",
    "./data/archive",
    "./data/get_available_data",
    "./data/get_available_data/gefs_reforecast",
    "./data/get_available_data/gefs_reforecast/nwp",
    "./data/spatialmos_climatology",
    "./data/media",
    "./data/spool",
    "./data/static"
  ],
  "systemChecks": {
    "archive_folder__gefs_avgspr_forecast_p05": "10095",
    "archive_folder__lwd": "10095",
    "archive_folder__zamg": "10095",
    "get_lwd": "75",
    "get_zamg": "75",
    "get_gefs_forecasts__rh_2m_avg": "1455",
    "get_gefs_forecasts__rh_2m_spr": "1455",
    "get_gefs_forecasts__tmp_2m_avg": "1455",
    "get_gefs_forecasts__tmp_2m_spr": "1455",
    "get_gefs_forecasts__ugrd_10m_avg": "1455",
    "get_gefs_forecasts__ugrd_10m_spr": "1455",
    "get_gefs_forecasts__vgrd_10m_avg": "1455",
    "get_gefs_forecasts__vgrd_10m_spr": "1455",
    "merge_statusfiles": "16",
    "prediction__tmp_2m": "1455",
    "prediction__rh_2m": "1455",
    "pre_processing_gribfiles__rh-2m": "1455",
    "pre_processing_gribfiles__tmp-2m": "1455"
  },
  "REMOTE_USER": "example",
  "REMOTE_HOST": "example.com",
  "rsync_push": {
    "sourcefiles": {
      "local_dir": ".",
      "remote_dir": "/www/spatialMOS",
      "include": [
        "fabric/cert/ca.pem",
        "fabric/cert/cert.pem",
        "fabric/cert/key.pem",
        "container/py_container/spatial_rust_util-0.1.1-cp39-cp39-linux_x86_64.whl",
        "container/py_container/spatial_rust_util-0.1.1-cp310-cp310-linux_x86_64.whl",
        "website/dist",
        "settings.json",
        "task.py"
      ],
      "exclude": [
        "*.git*",
        ".pylintrc",
        ".editorconfig"
      ],
      "exclude-from": [
        ".gitignore"
      ]
    },
    "staticfiles": {
      "local_dir": "./data",
      "remote_dir": "/www/spatialMOS",
      "include": [
        "static"
      ],
      "exclude": [
        ".well-known",
        "*.h5",
        "*.RData",
        "*_station_observations.csv",
        "archive",
        "get_available_data",
        "media",
        "spatialmos_climatology",
        "spool"
      ]
    },
    "climatologies": {
      "local_dir": "./data",
      "remote_dir": "/www/spatialMOS/",
      "ignore-existing": "True",
      "include": [
        "get_available_data/gadm",
        "spatialmos_climatology/gam/"
      ],
      "exclude": [
        "get_available_data/*",
        "*.h5",
        "*.RData",
        "*_station_observations.csv",
        ".well-known",
        "archive",
        "media",
        "static",
        "spool"
      ]
    }
  },
  "rsync_get": {
    "archive": {
      "local_dir": "./data/archive",
      "remote_dir": "/www/spatialMOS/data/archive",
      "ignore-existing": "True"
    }
  }
}