{
  "collection": "development",
  "docker_compose_files": [
    "./docker-compose.yml"
  ],
  "initFolders": [
    "./data/.well-known",
    "./data/archive",
    "./data/get_available_data",
    "./data/get_available_data/gefs_reforecast",
    "./data/get_available_data/gefs_reforecast/nwp",
    "./data/spatialmos_climatology",
    "./data/media",
    "./data/spool",
    "./data/static"
  ],
  "systemChecks": {
    "py_spatialmos__merge_statusfiles": "30",
    "py_spatialmos__available_systemchecks": "30",
    "py_spatialmos__get_lwd__get_lwd_data": "75",
    "py_spatialmos__get_zamg__get_zamg_data": "75",
    "py_spatialmos__get_gefs__get_gefs_forecasts__0_5__avg__ugrd_10m": "1455",
    "py_spatialmos__get_gefs__get_gefs_forecasts__0_5__avg__vgrd_10m": "1455",
    "py_spatialmos__get_gefs__get_gefs_forecasts__0_5__avg__rh_2m": "1455",
    "py_spatialmos__get_gefs__get_gefs_forecasts__0_5__avg__tmp_2m": "1455",
    "py_spatialmos__get_gefs__get_gefs_forecasts__0_5__spr__rh_2m": "1455",
    "py_spatialmos__get_gefs__get_gefs_forecasts__0_5__spr__tmp_2m": "1455",
    "py_spatialmos__get_gefs__get_gefs_forecasts__0_5__spr__ugrd_10m": "1455",
    "py_spatialmos__get_gefs__get_gefs_forecasts__0_5__spr__vgrd_10m": "1455",
    "py_spatialmos__prediction__prediction__0_5__tmp_2m": "1455",
    "py_spatialmos__prediction__prediction__0_5__rh_2m": "1455",
    "py_spatialmos__pre_processing_gribfiles__pre_processing_prediction__0_5__tmp_2m": "1455",
    "py_spatialmos__pre_processing_gribfiles__pre_processing_prediction__0_5__rh_2m": "1455"
  },
  "REMOTE_USER": "example",
  "REMOTE_HOST": "example.com",
  "rsync_push": {
    "sourcefiles": {
      "local_dir": ".",
      "remote_dir": "/www/spatialMOS",
      "include": [
        "fabric/cert/ca.pem",
        "fabric/cert/cert.pem",
        "fabric/cert/key.pem",
        "container/py_container/spatial_rust_util-0.1.1-cp39-cp39-linux_x86_64.whl",
        "container/py_container/spatial_rust_util-0.1.1-cp310-cp310-linux_x86_64.whl",
        "website/dist",
        "settings.json",
        "task.py"
      ],
      "exclude": [
        "*.git*",
        ".pylintrc",
        ".editorconfig"
      ],
      "exclude-from": [
        ".gitignore"
      ]
    },
    "staticfiles": {
      "local_dir": "./data",
      "remote_dir": "/www/spatialMOS",
      "include": [
        "static"
      ],
      "exclude": [
        ".well-known",
        "*.h5",
        "*.RData",
        "*_station_observations.csv",
        "archive",
        "get_available_data",
        "media",
        "spatialmos_climatology",
        "spool"
      ]
    },
    "climatologies": {
      "local_dir": "./data",
      "remote_dir": "/www/spatialMOS/",
      "ignore-existing": "True",
      "include": [
        "get_available_data/gadm",
        "spatialmos_climatology/gam/"
      ],
      "exclude": [
        "get_available_data/*",
        "*.h5",
        "*.RData",
        "*_station_observations.csv",
        ".well-known",
        "archive",
        "media",
        "static",
        "spool"
      ]
    }
  },
  "rsync_get": {
    "archive": {
      "local_dir": "./data/archive",
      "remote_dir": "/www/spatialMOS/data/archive",
      "ignore-existing": "True"
    }
  }
}