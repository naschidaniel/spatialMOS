 20  6  *  *  * sh /www/spatialMOS/processing/py_get_available_data/spatialmos__py_get_available_data__gefs__ugrd_10m.sh >> /var/log/webserver/spatialmos__py_get_available_data__gefs__ugrd_10m.log 2>&1
 30  6  *  *  * sh /www/spatialMOS/processing/py_get_available_data/spatialmos__py_get_available_data__gefs__vgrd_10m.sh >> /var/log/webserver/spatialmos__py_get_available_data__gefs__vgrd_10m.log 2>&1

